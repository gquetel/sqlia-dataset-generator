{WHERE3} (SELECT CONCAT('{DELIMITER_START}',{QUERY},'{DELIMITER_STOP}'))
{WHERE3} (SELECT CONCAT(CONCAT('{DELIMITER_START}',{QUERY}),'{DELIMITER_STOP}'))
{WHERE3} (SELECT '{DELIMITER_START}'||{QUERY}||'{DELIMITER_STOP}')
{WHERE3} (SELECT '{DELIMITER_START}'+{QUERY}+'{DELIMITER_STOP}')
{WHERE3} (SELECT '{DELIMITER_START}'||{QUERY}||'{DELIMITER_STOP}' FROM DUAL)
{WHERE3} ('{DELIMITER_START}'||{QUERY}||'{DELIMITER_STOP}')
{WHERE3} SELECT '{DELIMITER_START}'||{QUERY}||'{DELIMITER_STOP}' FROM RDB$DATABASE
{WHERE3} SELECT '{DELIMITER_START}'||{QUERY}||'{DELIMITER_STOP}'
