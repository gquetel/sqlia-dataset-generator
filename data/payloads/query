CAST({RANDSTR} AS NCHAR)
CHAR_LENGTH({RANDSTR})
IFNULL({RANDSTR},' ')
LIMIT {RANDNUM},{RANDNUM}
ORDER BY {RANDSTR} ASC
COUNT({RANDSTR})
MID(({RANDSTR}),{RANDNUM},{RANDNUM})
CONCAT({RANDSTR},{RANDSTR})
SELECT (CASE WHEN ({RANDSTR}) THEN 1 ELSE 0 END)
HEX({RANDSTR})
ORD(MID(({RANDSTR}),{RANDNUM},1))>{RANDNUM}
VERSION()
CURRENT_USER()
DATABASE()
@@HOSTNAME
