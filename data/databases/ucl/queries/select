SELECT {!column_name}, {!column_name} FROM {!table_name} WHERE {!column_name}='{nationality}'
select {!column_name} from {!table_name} where {!column_name}="{club_name}" or {!column_name}={nb_shoot};
select * from {!table_name} where {!column_name}='{player_position}' UNION select {!column_name}, {!column_name} from {!table_name} where {!column_name}>{goalkeper_succes} or {!column_name}>{nb_takkles};
SELECT * FROM {!table_name} WHERE {column_name} <  {nb_touches}
SELECT {!column_name}, {!column_name} FROM {!table_name} WHERE {!column_name}>{total_playing_time} AND {!column_name}<{player_age}
SELECT {!column_name} FROM {!table_name} WHERE {!column_name}='{player_position}'  AND NOT {!column_name}='{player_name}'
