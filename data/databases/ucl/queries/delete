delete  from {!table_name} where {!column_name}='{player_name}';
DELETE from {!table_name} WHERE {!column_name}="{nationality}" AND {!column_name}="{club_name}";
delete from {table_name};
DELETE FROM {!table_name} WHERE {!column_name} IN (SELECT {!column_name} FROM {!table_name} WHERE {!column_name} = {total_playing_time});
DELETE FROM {!table_name} WHERE {!column_name} IN ((SELECT {!column_name} FROM {!table_name} WHERE {!column_name} = "{club_name}" AND NOT  {!column_name} > {nb_fouls})  UNION (SELECT {!column_name} FROM {!table_name} WHERE {!column_name} = {pass_completion}));
DELETE from {!table_name} WHERE {!column_name} > {nb_pass_de} AND {!column_name} > {pass_completion} AND {!column_name} > {total_passing_dist};
